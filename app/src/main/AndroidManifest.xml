<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zhang.keybox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Key Box">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".EditorActivity">
        </activity>
        <activity android:name=".ShowActivity">
        </activity>
        <activity android:name=".drawer.SettingActivity">
        </activity>
        <activity android:name=".drawer.SourceActivity">
        </activity>
        <activity android:name=".drawer.ReplaceActivity">
        </activity>
        <activity android:name=".drawer.ImportActivity">
        </activity>
        <activity android:name=".drawer.ExportActivity">
        </activity>
        <activity
            android:name=".drawer.set.HelpActivity"
            android:label="@string/title_activity_help"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".drawer.set.AboutActivity">
        </activity>
    </application>

</manifest>